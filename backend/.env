NODE_ENV=development
PORT=5000
HOST=localhost

# Use SQLite for quick local dev
DB_DIALECT=sqlite
SQLITE_STORAGE=./data/dev2.sqlite

# CORS and Auth
CORS_ORIGIN=http://localhost:3000
JWT_SECRET=dev_secret
